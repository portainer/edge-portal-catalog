{
    "flowFile": "flows.json",
    "credentialSecret": "your-strong-secret",
    "ui": {
      "path": "ui"
    },
    "httpAdminRoot": "/admin",
    "httpNodeRoot": "/api",
    "userDir": "/data",
    "functionGlobalContext": {},
    "logging": {
      "console": {
        "level": "info",
        "metrics": false,
        "audit": false
      }
